# AWS
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_DEFAULT_REGION=us-east-1
S3_BUCKET_NAME=your-bucket-name

# Snowflake (PAT authentication)
SNOWFLAKE_ORGANIZATION_NAME=your-org-name
SNOWFLAKE_ACCOUNT_NAME=your-account-name
SNOWFLAKE_USER=your-username
SNOWFLAKE_TOKEN=your-pat-token

# Snowflake resources (optional, defaults shown)
SNOWFLAKE_WAREHOUSE=DATA_WH
SNOWFLAKE_WAREHOUSE_SIZE=XSMALL
SNOWFLAKE_AUTO_SUSPEND=60
SNOWFLAKE_MIN_CLUSTER_COUNT=1
SNOWFLAKE_MAX_CLUSTER_COUNT=1
SNOWFLAKE_DATABASE=SHELTER_DB
SNOWFLAKE_STORAGE_INTEGRATION=SNOWFLAKE_INTEGRATION

# AWS IAM (optional, defaults shown)
S3_IAM_POLICY_NAME=s3_access
S3_IAM_USER_NAME=s3_user
SNOWFLAKE_IAM_ROLE_NAME=snowflake_s3_access
